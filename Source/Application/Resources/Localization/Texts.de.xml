<cultures>
	<culture name="de">
		<common>
			<culture>Kultur</culture>
			<sign-in>Anmelden</sign-in>
			<sign-out>Abmelden</sign-out>
			<signing-in-with>Anmeldung mit</signing-in-with>
			<site-name>Anwendung</site-name>
			<ui-culture>UI-Kultur</ui-culture>
			<users>Benutzer</users>
			<your-identity>Ihre Identität</your-identity>
		</common>
		<pages>
			<account>
				<accessDenied>
					<index>
						<heading>Zugriff verweigert</heading>
						<information>Sie haben keinen Zugriff auf die Ressource, die Sie zu erreichen versuchen.</information>
						<title lookup="pages.account.accessdenied.index.heading" />
					</index>
				</accessDenied>
				<index>
					<claims>Ansprüche</claims>
					<heading lookup="common.your-identity" />
					<properties>Eigenschaften</properties>
					<title lookup="common.your-identity" />
					<user-name>Benutzername</user-name>
				</index>
				<signedOut>
					<index>
						<heading>Abgemeldet</heading>
						<information>Sie sind jetzt abgemeldet.</information>
						<return-to-previous-page>Zurück zur vorherigen Seite.</return-to-previous-page>
						<title lookup="pages.account.signedout.index.heading" />
					</index>
				</signedOut>
				<signIn>
					<index>
						<form>
							<error>
								<invalid-credentials>&quot;{0}&quot; oder/und &quot;{1}&quot; ist falsch.</invalid-credentials>
								<required-format>&quot;{0}&quot; ist erforderlich.</required-format>
							</error>
							<heading>Formular-Authentifizierung</heading>
							<information>Melden Sie sich mit Benutzername und Passwort an.</information>
							<password>Passwort</password>
							<username>Benutzername</username>
						</form>
						<heading>Anmelden</heading>
						<idp>
							<heading>Duende IdentityServer Demo</heading>
							<information>Melden Sie sich mit dem Duende IdentityServer Demo an.</information>
						</idp>
						<information>Wählen Sie eine der folgenden Alternativen:</information>
						<sign-in-with>Anmelden mit</sign-in-with>
						<title lookup="pages.account.signin.index.heading" />
					</index>
				</signIn>
				<signOut>
					<index>
						<confirm>Ja</confirm>
						<confirm-question>Möchten Sie sich abmelden?</confirm-question>
						<heading lookup="common.sign-out" />
						<title lookup="common.sign-out" />
					</index>
				</signOut>
				<users>
					<index>
						<heading lookup="common.users" />
						<information>Benutzer suchen. Sie können &quot;*&quot; als Platzhalter verwenden, wenn Sie suchen.</information>
						<no-hit>Kein Treffer.</no-hit>
						<reset>Zurücksetzen</reset>
						<result>Ergebnis</result>
						<search>Suchen</search>
						<title lookup="common.users" />
						<user>
							<email>E-Mail</email>
							<preferred-userName>Bevorzugter Benutzername</preferred-userName>
							<subject>Betreff (ID)</subject>
							<userName>Benutzername</userName>
						</user>
					</index>
				</users>
			</account>
			<index>
				<culture lookup="common.culture" />
				<heading>Startseite</heading>
				<information>Sie befinden sich auf der Startseite.</information>
				<title lookup="pages.index.heading" />
				<ui-culture lookup="common.ui-culture" />
			</index>
			<shared>
				<layout>
					<home>Startseite</home>
					<logotype>Logo</logotype>
					<select-culture>Sprache auswählen.</select-culture>
					<sign-in lookup="common.sign-in" />
					<sign-out lookup="common.sign-out" />
					<site-name lookup="common.site-name" />
					<toggle-navigation>Navigation umschalten</toggle-navigation>
					<users lookup="common.users" />
					<your-identity lookup="common.your-identity" />
				</layout>
				<validationSummary>
					<heading>Eingabefehler</heading>
					<information>Das Formular ist nicht korrekt ausgefüllt.</information>
				</validationSummary>
			</shared>
		</pages>
	</culture>
</cultures>