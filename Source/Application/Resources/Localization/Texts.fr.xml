<cultures>
	<culture name="fr">
		<common>
			<culture>Culture</culture>
			<sign-in>Se connecter</sign-in>
			<sign-out>Se déconnecter</sign-out>
			<signing-in-with>Connexion avec</signing-in-with>
			<site-name>Application</site-name>
			<ui-culture>Culture de l'interface</ui-culture>
			<users>Utilisateurs</users>
			<your-identity>Votre identité</your-identity>
		</common>
		<pages>
			<account>
				<accessDenied>
					<index>
						<heading>Accès refusé</heading>
						<information>Vous n'avez pas accès à la ressource que vous essayez d'atteindre.</information>
						<title lookup="pages.account.accessdenied.index.heading" />
					</index>
				</accessDenied>
				<index>
					<claims>Revendiquer</claims>
					<heading lookup="common.your-identity" />
					<properties>Propriétés</properties>
					<title lookup="common.your-identity" />
					<user-name>Nom d'utilisateur</user-name>
				</index>
				<signedOut>
					<index>
						<heading>Déconnecté</heading>
						<information>Vous êtes maintenant déconnecté.</information>
						<return-to-previous-page>Retourner à la page précédente.</return-to-previous-page>
						<title lookup="pages.account.signedout.index.heading" />
					</index>
				</signedOut>
				<signIn>
					<index>
						<form>
							<error>
								<invalid-credentials>&quot;{0}&quot; ou/et &quot;{1}&quot; est incorrect.</invalid-credentials>
								<required-format>&quot;{0}&quot; est requis.</required-format>
							</error>
							<heading>Authentification par formulaire</heading>
							<information>Connectez-vous avec un nom d'utilisateur et un mot de passe.</information>
							<password>Mot de passe</password>
							<username>Nom d'utilisateur</username>
						</form>
						<heading>Connexion</heading>
						<idp>
							<heading>Démo Duende IdentityServer</heading>
							<information>Connexion avec la démo Duende IdentityServer.</information>
						</idp>
						<information>Choisissez une des alternatives suivantes :</information>
						<sign-in-with>Se connecter avec</sign-in-with>
						<title lookup="pages.account.signin.index.heading" />
					</index>
				</signIn>
				<signOut>
					<index>
						<confirm>Oui</confirm>
						<confirm-question>Voulez-vous vous déconnecter ?</confirm-question>
						<heading lookup="common.sign-out" />
						<title lookup="common.sign-out" />
					</index>
				</signOut>
				<users>
					<index>
						<heading lookup="common.users" />
						<information>Rechercher des utilisateurs. Vous pouvez utiliser &quot;*&quot; comme caractère générique lors de la recherche.</information>
						<no-hit>Aucun résultat.</no-hit>
						<reset>Réinitialiser</reset>
						<result>Résultat</result>
						<search>Rechercher</search>
						<title lookup="common.users" />
						<user>
							<email>Email</email>
							<preferred-userName>Nom d'utilisateur préféré</preferred-userName>
							<subject>Sujet (id)</subject>
							<userName>Nom d'utilisateur</userName>
						</user>
					</index>
				</users>
			</account>
			<index>
				<culture lookup="common.culture" />
				<heading>Accueil</heading>
				<information>Vous êtes sur la page d'accueil.</information>
				<title lookup="pages.index.heading" />
				<ui-culture lookup="common.ui-culture" />
			</index>
			<shared>
				<layout>
					<home>Accueil</home>
					<logotype>Logotype</logotype>
					<select-culture>Choisir la langue.</select-culture>
					<sign-in lookup="common.sign-in" />
					<sign-out lookup="common.sign-out" />
					<site-name lookup="common.site-name" />
					<toggle-navigation>Basculer la navigation</toggle-navigation>
					<users lookup="common.users" />
					<your-identity lookup="common.your-identity" />
				</layout>
				<validationSummary>
					<heading>Erreur de saisie</heading>
					<information>Le formulaire n'est pas correctement rempli.</information>
				</validationSummary>
			</shared>
		</pages>
	</culture>
</cultures>